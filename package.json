{
  "name": "i18next-datasheet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.3.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.4.1",
    "classnames": "^2.2.6",
    "css-loader": "0.28.7",
    "dotenv": "6.1.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.6.0",
    "js-yaml": "^3.12.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.2",
    "prop-types": "^15.6.2",
    "raf": "3.4.1",
    "react": "^16.4.0",
    "react-datasheet": "^1.3.7",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "resolve": "1.8.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "23.10.4",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "TS_NODE_FILES=true ts-node scripts/start.ts",
    "build": "TS_NODE_FILES=true ts-node scripts/build.ts",
    "test": "TS_NODE_FILES=true ts-node scripts/test.ts --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "update": "yarn upgrade-interactive --latest"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addons": "^3.4.7",
    "@storybook/react": "^3.4.7",
    "@types/autoprefixer": "^6.7.3",
    "@types/body-parser": "^1.17.0",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/classnames": "^2.2.6",
    "@types/express": "^4.16.0",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/fs-extra": "^5.0.4",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^23.3.9",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.12.5",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__react": "^3.0.7",
    "@types/sw-precache-webpack-plugin": "^0.11.0",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.0",
    "@types/webpack-dev-server": "^2.9.4",
    "@types/webpack-manifest-plugin": "^1.3.2",
    "babel-core": "6",
    "babel-runtime": "^6.26.0",
    "prettier": "^1.15.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
